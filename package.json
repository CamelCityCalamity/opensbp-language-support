{
  "name": "opensbp-language-support",
  "displayName": "OpenSBP",
  "publisher": "CamelCityCalamity",
  "description": "Support for the OpenSBP language",
  "icon": "icon.png",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/camelcitycalamity/opensbp-language-support.git"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "opensbp",
        "aliases": [
          "OpenSBP",
          "opensbp"
        ],
        "extensions": [
          ".sbp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "opensbp",
        "scopeName": "source.opensbp",
        "path": "./syntaxes/opensbp.tmLanguage.json"
      }
    ]
  }
}